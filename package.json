    "start": "eleventy --serve",
    "watch:css": "npx @tailwindcss/cli -i ./src/assets/styles/base.css -o ./dist/assets/styles/base.css --watch",
    "dev": "npm-run-all --parallel watch:css start",
    "build": "eleventy",
    "build:css": "npx @tailwindcss/cli -i ./src/assets/styles/base.css -o ./dist/assets/styles/base.css --minify"
  },
